{
    "name": "tautvydasr/blade-class-names",
    "description": "Package for handling html classes output based on conditions",
    "keywords": [
        "class-names",
        "laravel",
        "blade"
    ],
    "license": "MIT",
    "support": {
        "source": "https://github.com/tautvydasr/blade-class-names",
        "issues": "https://github.com/tautvydasr/blade-class-names/issues"
    },
    "authors": [
        {
            "name": "Tautvydas Rasimavicius",
            "email": "tautvydas.rasimavicius@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "^5.6 || 6.* || 7.* || 8.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "ClassNames\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ClassNames\\ClassNamesServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
